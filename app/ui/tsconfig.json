{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "ES2023",
		"module": "ES2022",
		"moduleResolution": "node",
		"rootDir": "./webapp",
		"outDir": "./dist",
		"lib": ["ES2023", "DOM"],
		"types": ["@sapui5/types", "@types/qunit"],
		"paths": {
			"ui/*": ["./webapp/*"],
			"unit/*": ["./webapp/test/unit/*"],
			"integration/*": ["./webapp/test/integration/*"]
		}
	},
	"include": ["./webapp"]
}
