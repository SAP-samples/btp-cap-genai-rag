{
    "cdsc": {
        "beta": {
            "vectorType": true
        }
    },
    "requires": {
        "PROVIDER_AI_CORE_DESTINATION_HUB": {
            "kind": "rest",
            "credentials": {
                "destination": "PROVIDER_AI_CORE_DESTINATION_HUB",
                "requestTimeout": "300000"
            }
        },
        " ---------------------": "---",
        "--- MTXs Settings ----": "---",
        "--------------------- ": "---",
        "[production]": {
            "multitenancy": true,
            "toggles": true,
            "extensibility": true,
            "cds.xt.ExtensibilityService": {
                "element-prefix": ["x_"],
                "extension-allowlist": [
                    {
                        "for": ["*"]
                    }
                ]
            }
        }
    }
}
