{
    "cdsc": {
        "beta": {
            "vectorType": true
        }
    },
    "requires": {
        "PROVIDER_AI_CORE_DESTINATION_HUB": {
            "kind": "rest",
            "credentials": {
                "destination": "PROVIDER_AI_CORE_DESTINATION_HUB",
                "requestTimeout": "300000"
            }
        },
        "multitenancy": true,
        "toggles": true,
        "extensibility": false,
        "db": {
            "kind": "hana",
            "impl": "@sap/cds-hana",
            "deploy-format": "hdbtable",
            "vcap": {
                "label": "service-manager",
                "plan": "container"
            }
        }
    }
}
