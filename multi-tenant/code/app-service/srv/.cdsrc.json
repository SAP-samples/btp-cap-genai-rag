{
    "requires": {
        "multitenancy": true,
        "toggles": true,
        "extensibility": true,
        "cds.xt.ExtensibilityService": {
            "element-prefix": [
                "x_"
            ],
            "extension-allowlist": [
                {
                    "for": [
                        "*"
                    ]
                }
            ]
        },
        "PROVIDER_AI_CORE_DESTINATION": {
            "kind": "rest",
            "credentials": {
                "destination": "PROVIDER_AI_CORE_DESTINATION"
            }
        },
        "SUBSCRIBER_OFFICE365_DESTINATION": {
            "kind": "rest",
            "credentials": {
                "destination": "SUBSCRIBER_OFFICE365_DESTINATION"
            }
        },
        "aicore": {
            "createResourceGroups": false
        },
        "postgres": {
            "kind": "postgresql-db"
        },
        "hana": {
            "kind": "hana-cloud",
            "vcap": {
                "name": "hana",
                "tags": ["hana"],
                "plan": "hdi-shared",
                "label": "hana"
            }
        },
        "db": {
            "kind": "hana", 
            "impl": "@sap/cds-hana",
            "deploy-format": "hdbtable",
            "vcap": {
                "label": "service-manager",
                "plan": "container"
            }
        },
        "auth": {
            "kind": "xsuaa"
        }
    }
}