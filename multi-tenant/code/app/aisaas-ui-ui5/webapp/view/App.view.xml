<mvc:View
    controllerName="aisaas.ui.controller.App"
    displayBlock="true"
    xmlns="sap.f"
    xmlns:m="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:mvc="sap.ui.core.mvc"
>
    <l:FixFlex>
        <l:fixContent>
            <ShellBar title="{i18n>appTitle}" homeIcon="images/sap-logo.svg" />
        </l:fixContent>
        <l:flexContent>
            <m:App id="app" />
        </l:flexContent>
    </l:FixFlex>
</mvc:View>
