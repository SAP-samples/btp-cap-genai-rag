{
    "name": "aisaas",
    "version": "0.0.1",
    "repository": "https://github.com/SAP-samples/btp-cap-multitenant-ai",
    "license": "Apache-2.0",
    "description": "AI SaaS",
    "engines": {
        "node": "^18.x"
    },
    "private": true,
    "workspaces": [
        "srv",
        "api",
        "common/dist"
    ],
    "dependencies": {
        "@kubernetes/client-node": "^0.18.1",
        "@sap-cloud-sdk/connectivity": "^3.2.0",
        "@sap-cloud-sdk/generator": "^3.4.0",
        "@sap-cloud-sdk/http-client": "^3.2.0",
        "@sap-cloud-sdk/openapi": "^3.2.0",
        "@sap/audit-logging": "^5",
        "@sap/cds": "^7.1.1",
        "@sap/cds-hana": "^2.0.0",
        "@sap/cds-mtxs": "^1.10.0",
        "@sap/cds-odata-v2-adapter-proxy": "^1",
        "@sap/xsenv": "^3.4.0",
        "@sap/xssec": "^3.2.17",
        "@swc/core": "^1.3.78",
        "@sap/llm-commons": "*",
        "axios": "^0.27.2",
        "cds-swagger-ui-express": "^0.5.0",
        "cf-nodejs-logging-support": "^6",
        "cfenv": "^1",
        "cors": "^2.8.5",
        "express": "^4",
        "langchain": "^0.0.122",
        "node-fetch": "^2.6.7",
        "node-jose": "^2.1",
        "passport": "^0.6.0",
        "pg": "^8.11.2",
        "typeorm": "^0.3.17"
    },
    "devDependencies": {
        "@sap/approuter": "^14.1.1",
        "@sap/cds-common-content": "1.0.1",
        "@sap/cds-dk": "^7.1.1",
        "@sap/html5-app-deployer": "^5.0.0",
        "@sap/html5-repo-mock": "^2.1.1",
        "@sap/sbf": "^6.5.2",
        "@sap/ux-specification": "^1.96.4",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.2",
        "@types/pg": "^8.10.2",
        "@types/sap__xsenv": "^3.3.0",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "chai-subset": "^1.6.0",
        "jest": "^29.0.1",
        "mbt": "^1.2.18",
        "rimraf": "^5.0.1",
        "shx": "^0.3.4",
        "sqlite3": "^5.1.6",
        "ui5-task-zipper": "^0.5.1"
    },
    "scripts": {
        "start": "cds-serve",
        "deploy": "cds deploy --to sqlite",
        "----------------------": "",
        "-Local/Hybrid-Testing-": "",
        "/--------------------/": "",
        "test": "mocha --timeout 20000",
        "---API-Service---": "",
        "api:watch": "(cd api-service && cds-ts watch --profile development)",
        "api:mtx": "(cd api-service && cds-ts watch --profile local-with-mtx)",
        "api:hybrid": "(cd api-service && cds-ts watch --profile hybrid)",
        "---Backend-Service---": "",
        "srv:watch": "(cd app-service && cds-ts watch --profile development)",
        "srv:mtx": "(cd app-service && cds-ts watch --profile local-with-mtx)",
        "srv:hybrid": "(cd app-service && cds-ts watch --profile hybrid)",
        "srv:bas": "(cd app-service && cds-ts watch --profile bas)",
        "---Service-Broker---": "",
        "broker:hybrid": "cds bind --exec npm run broker:run",
        "broker:run": "(cd broker && cds bind --exec --profile hybrid npm start)",
        "---HTML5-Mock-Repo---": "",
        "mock:watch": "cds bind --exec npm run mock:run:watch",
        "mock:hybrid": "cds bind --exec npm run mock:run:hybrid",
        "mock:run:watch": "(cd router && cds bind --exec --profile development node ../node_modules/@sap/html5-repo-mock/index.js)",
        "mock:run:hybrid": "(cd router && cds bind --exec --profile hybrid node ../node_modules/@sap/html5-repo-mock/index.js)",
        "---Application-Router---": "",
        "router:hybrid": "cds bind --exec npm run router:run",
        "router:run": "(cd router && cds bind --exec --profile hybrid node ../node_modules/@sap/approuter/approuter.js)",
        "-Manual HTML5 Deployment-": "",
        "deployer:run": "(cd app/html5-deployer && cds bind --exec --profile hybrid node ../../node_modules/@sap/html5-app-deployer/index.js)",
        "deployer:hybrid": "cds bind --exec npm run deployer:run",
        "---------------------": "",
        "-----PostgreSQL------": "",
        "/-------------------/": "",
        "start:pg": "docker compose up -d",
        "stop:pg": "docker compose stop",
        "--------------------": "",
        "--------BUILD-------": "",
        "/------------------/": "",
        "build": "npm run build:deps && npm run build:cds && npm run build:ts && npm run cleanup:ts",
        "build:ts": "tsc -b app-service && tsc -b api-service",
        "build:cds": "cds build --production",
        "build:deps": "npm i",
        "cleanup:ts": "npx shx rm -r './gen/**/*.{ts,tsconfig}'",
        "cds2types": "cd app-service && cds2types --cds ./$npm_config_service.cds --output ./types/entities.ts"
    },
    "eslintConfig": {
        "extends": "eslint:recommended",
        "env": {
            "es2020": true,
            "node": true,
            "jest": true,
            "mocha": true
        },
        "globals": {
            "SELECT": true,
            "INSERT": true,
            "UPDATE": true,
            "DELETE": true,
            "CREATE": true,
            "DROP": true,
            "CDL": true,
            "CQL": true,
            "CXL": true,
            "cds": true
        },
        "rules": {
            "no-console": "off",
            "require-atomic-updates": "off"
        }
    },
    "prettier": {
        "arrowParens": "always",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "none",
        "printWidth": 120
    },
    "cds": {
        "folders": {
            "db-sqlite": "db/sqlite",
            "app-srv": "app-service/srv",
            "api-srv": "api-service/srv"
        },
        "build": {
            "tasks": [
                {
                    "for": "hana",
                    "options": {
                        "model": [
                            "db",
                            "db/hana",
                            "api-service/srv",
                            "app-service/srv",
                            "@sap/cds-mtxs/db/extensions"
                        ]
                    }
                },
                {
                    "for": "nodejs",
                    "src": "app-service/srv",
                    "dest": "srv",
                    "options": {
                        "model": [
                            "app-service/srv",
                            "@sap/cds-mtxs/srv/bootstrap",
                            "@sap/cds-mtxs/srv/extensibility-service"
                        ]
                    }
                },
                {
                    "for": "nodejs",
                    "src": "api-service/srv",
                    "dest": "api",
                    "options": {
                        "model": "api-service/srv"
                    }
                },
                {
                    "for": "mtx",
                    "src": "app-service/srv",
                    "dest": "srv"
                },
                {
                    "for": "hana",
                    "src": "db-com",
                    "options": {
                        "model": "db-com"
                    }
                }
            ]
        }
    },
    "sapux": [
        "app/ui-flp"
    ]
}
