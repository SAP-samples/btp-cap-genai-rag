{
  "welcomeFile": "/index.html",
  "logout": {
    "logoutEndpoint": "/logout"
  },
  "routes": [
    {
      "source": "^/api/odata/v4/mail-insights/(.*)$",
      "target": "/catalog/PublicService/$1",
      "destination": "aisaas-srv-api",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "destination": "ui",
      "csrfProtection": false
    }
  ]
}