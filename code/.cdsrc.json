{
    "requires": {
        " ---------------------": "---",
        "--- MTXs Settings ----": "---",
        "--------------------- ": "---",
        "[hybrid]": {
            "multitenancy": true,
            "extensibility": true,
            "toggles": true,
            "cds.xt.ExtensibilityService": {
                "element-prefix": [
                    "x_"
                ],
                "extension-allowlist": [
                    {
                        "for": [
                            "*"
                        ]
                    }
                ]
            },
            "PROVIDER_AI_CORE_DESTINATION": {
                "kind": "rest",
                "credentials": {
                    "destination": "PROVIDER_AI_CORE_DESTINATION"
                }
            }
        },
        "[local-with-mtx]": {
            "multitenancy": true,
            "extensibility": true,
            "toggles": true
        },
        "[development]": {
            "multitenancy": false,
            "extensibility": false,
            "toggles": false
        },
        "------------------ ": "---",
        "--- DB Settings ---": "---",
        " ------------------": "---",
        "db": {
            "[hybrid]": {
                "kind": "hana",
                "vcap": {
                    "label": "service-manager",
                    "plan": "container"
                }
            },
            "[local-with-mtx]": {
                "kind": "sql-mt",
                "beta": {
                    "sqlMigration": "true"
                }
            },
            "[development]": {
                "kind": "sql"
            }
        },
        "-------------------- ": "---",
        "--- Auth Settings ---": "---",
        " --------------------": "---",
        "auth": {
            "[hybrid]": {
                "kind": "xsuaa"
            },
            "[local-with-mtx]": {
                "kind": "mocked",
                "users": {
                    "alice": {
                        "tenant": "t1",
                        "roles": ["Admin", "cds.Subscriber", "cds.ExtensionDeveloper"],
                        "password": "helloworld"
                    },
                    "carol": {
                        "tenant": "t1",
                        "roles": ["Member"],
                        "password": "helloworld"
                    },
                    "bob": {
                        "tenant": "t1-ext",
                        "roles": ["Admin", "cds.Subscriber", "cds.ExtensionDeveloper"],
                        "password": "helloworld"
                    }
                }
            },
            "[development]": {
                "kind": "mocked",
                "users": {
                    "alice": {
                        "roles": [
                            "Admin",
                            "Member"
                        ]
                    },
                    "carol": {
                        "roles": [
                            "Member"
                        ]
                    }
                }
            }
        }
    }
}
