{
    "requires": {
        " ---------------------": "---",
        "--- MTXs Settings ----": "---",
        "--------------------- ": "---",
        "PROVIDER_AI_CORE_DESTINATION": {
            "kind": "rest",
            "credentials": {
                "destination": "PROVIDER_AI_CORE_DESTINATION"
            }
        },
        "[hybrid]": {
            "multitenancy": true,
            "extensibility": true,
            "toggles": true,
            "cds.xt.ExtensibilityService": {
                "element-prefix": ["x_"],
                "extension-allowlist": [
                    {
                        "for": ["*"]
                    }
                ]
            },
            "PROVIDER_AI_CORE_DESTINATION": {
                "kind": "rest",
                "credentials": {
                    "destination": "PROVIDER_AI_CORE_DESTINATION"
                }
            },
            "postgres": {
                "kind": "postgresql-db"
            }
        },
        "[local-with-mtx]": {
            "multitenancy": true,
            "extensibility": true,
            "toggles": true,
            "postgres": {
                "kind": "postgres",
                "credentials": {
                  "hostname": "localhost",
                  "port": 5432,
                  "username": "postgres",
                  "password": "postgres",
                  "dbname": "postgres"
                }
            }
        },
        "[development]": {
            "multitenancy": false,
            "extensibility": false,
            "toggles": false,
            "postgres": {
              "kind": "postgres",
              "credentials": {
                "hostname": "localhost",
                "port": 5432,
                "username": "postgres",
                "password": "postgres",
                "dbname": "postgres"
              }
            }
        },
        "------------------ ": "---",
        "--- DB Settings ---": "---",
        " ------------------": "---",
        "db": {
            "[hybrid]": {
                "kind": "hana",
                "vcap": {
                    "label": "service-manager",
                    "plan": "container"
                }
            },
            "[local-with-mtx]": {
                "kind": "sql-mt",
                "beta": {
                    "sqlMigration": "true"
                }
            },
            "[development]": {
                "kind": "sql"
            }
        },
        "-------------------- ": "---",
        "--- Auth Settings ---": "---",
        " --------------------": "---",
        "auth": {
            "[hybrid]": {
                "kind": "xsuaa"
            },
            "[local-with-mtx]": {
                "kind": "mocked",
                "users": {
                    "alice": {
                        "tenant": "t1",
                        "roles": ["Admin", "cds.Subscriber", "cds.ExtensionDeveloper"]
                    },
                    "carol": {
                        "tenant": "t1",
                        "roles": ["Member"]
                    },
                    "bob": {
                        "tenant": "t1-ext",
                        "roles": ["Admin", "cds.Subscriber", "cds.ExtensionDeveloper"]
                    }
                }
            },
            "[development]": {
                "kind": "mocked",
                "users": {
                    "alice": {
                        "roles": ["Admin", "Member"]
                    },
                    "carol": {
                        "roles": ["Member"]
                    }
                }
            }
        }
    }
}
