{
    "compilerOptions": {
        "target": "ES2023",
        "module": "ESNext",
        "moduleResolution": "Node",
        "outDir": "./gen/srv/srv",
        "skipLibCheck": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "allowJs": true,
        "strict": true,
        "strictNullChecks": false,
        "strictPropertyInitialization": false,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "paths": {
            "#cds-models/*": ["./@cds-models/*/index.ts"]
        }
    },
    "include": ["./srv/**/*"]
}
