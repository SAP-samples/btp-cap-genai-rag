        "shx": "^0.3.4",
        "@cap-js/cds-typer": "^0"
    },
    "scripts": {
        "start": "cds-serve",
        "--------DEVELOPMENT-------": "",
        "watch": "npx --yes npm-run-all --npm-path npm --parallel cap router ui5",
        "ui5": "npm run start:hybrid --prefix ./app/ai-ui-ui5",
        "cap": "cds-ts watch --profile hybrid",
        "router": "npm run start:local --prefix ./router",
        "--------UI DEPLOYMENT-------": "",        
        "build": "npx -p @sap/cds-dk cds build --profile production",
        "deployer:hybrid": "cds bind --exec --profile ui-deployment npm run deployer:run",
        "deployer:run": "(cd app/html5-deployer && node ../../node_modules/@sap/html5-app-deployer/index.js)"
    },
    "prettier": {
        "arrowParens": "always",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": false,
        "trailingComma": "none",
        "printWidth": 120
    },
    "imports": {
        "#cds-models/*": "./@cds-models/*/index.js"
    }
}
