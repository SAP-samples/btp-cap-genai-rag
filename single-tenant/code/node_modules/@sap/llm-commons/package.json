{
  "name": "@sap/llm-commons",
  "version": "0.8.1",
  "description": "Commons JS/TS library for building Generative AI apps on SAP BTP",
  "type": "module",
  "main": "./build/cjs/index.cjs",
  "module": "build/esm/index.mjs",
  "types": "./index.d.ts",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "scripts": {
    "build": "rimraf ./build && rollup --config",
    "bundle": "rollup --config",
    "test": "echo \"skipping tests...\"",
    "test:skipped": "vitest run test/**/*",
    "test:unit": "vitest test/unit/**/*",
    "test:integration": "vitest test/integration/**/*",
    "test:single": "vitest",
    "lint": "prettier -c . && eslint .",
    "ci-lint": "npm run lint",
    "format": "prettier -w . && eslint --fix .",
    "docs": "typedoc ./src && concat-md --decrease-title-levels docs/api > docs/api/README.md",
    "version": "npm i && npm run build && conventional-changelog -p angular -i CHANGELOG.md -s && npm run docs && git add .",
    "clean": "rimraf ./build",
    "ci-init": "sh ./.pipeline/init.sh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.tools.sap/AI-Playground-Projects/llm-commons-js.git"
  },
  "keywords": [
    "llm",
    "chatgpt",
    "genai",
    "ai",
    "btp",
    "tools"
  ],
  "author": "merbin.j.anselm@sap.com",
  "engines": {
    "node": "^18.0.0 || ^20.0.0"
  },
  "devDependencies": {
    "@google-ai/generativelanguage": "^0.2.1",
    "@tsconfig/node18": "^18.2.1",
    "@types/sap__xsenv": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "@vitest/coverage-v8": "^0.34.3",
    "concat-md": "^0.5.1",
    "conventional-changelog-cli": "^4.0.0",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.28.1",
    "rollup-plugin-dts": "^6.0.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "typedoc": "^0.25.0",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3"
  },
  "dependencies": {
    "@sap-cloud-sdk/util": "^3.4.0",
    "@sap/xsenv": "^4.0.0",
    "axios": "^1.5.0",
    "zod-to-json-schema": "^3.21.4"
  },
  "files": [
    "build",
    "client",
    "langchain",
    "index.d.ts",
    "config-*.d.ts",
    "base-*.d.ts"
  ],
  "peerDependencies": {
    "langchain": "~0.0.119"
  },
  "peerDependenciesMeta": {
    "langchain": {
      "optional": true
    }
  },
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./build/esm/index.mjs",
      "require": "./build/cjs/index.cjs"
    },
    "./client/btp": {
      "types": "./client/btp.d.ts",
